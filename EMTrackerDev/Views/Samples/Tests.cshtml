@model IEnumerable<EMTrackerDev.Models.TestResult>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<h1>Reports for @ViewBag.Id</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AnalysisReportId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EnteredBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Test)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AnalysisReportId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.StartDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EndDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EnteredBy.UserId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Test.TestID)
                </td>
                <td>
                    <a asp-action="Edit_TestResults" asp-route-id="@item.TestResultId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.TestResultId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.TestResultId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
