@model IEnumerable<EMTrackerDev.Models.Result>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ResultName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UOM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ResultDateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sample.SampleName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ResultName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UOM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResultDateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sample.SampleName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ResultID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ResultID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ResultID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
