@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EMTrackerDevHome</title>
    <script src="https://kit.fontawesome.com/dcfe4a17db.js" crossorigin="anonymous"></script>

</head>
<body>
    <div class="text-center">
        <h1 class="display-4">Welcome *User*</h1>
    </div><br /><br />
    <div class="container">
      <div class="row">
              <div class="col-sm-4">
            </div>
              <div class="text-center">

                  <div class="row-cols-sm-4">
                      <strong>Create</strong>
                      <button>
                          
                          <!-- solid style -->
                          <<i class="fas fa-plus"></i>
                      </button>

                  </div>
                  <div class="row-cols-sm-4">
                      <strong>Collect</strong>
                      <button>
                          <!-- solid style -->
                          <i class="fas fa-truck"></i>
                      </button>
                  </div>
                  <div class="row-cols-sm-4">
                      <strong>Analyze</strong>
                      <!-- solid style -->
                      <button>
                          <i class="fas fa-vials"></i>
                      </button>

                  </div>
                  <div class="row-cols-sm-4">
                      <strong>Enter Results</strong>
                      <!-- solid style -->
                      <button>
                          <i class="fas fa-file-alt"></i>
                      </button>
                  </div>
                  <div class="row-cols-sm-4">
                      <strong>Approve</strong>
                      <!-- solid style -->
                      <button>
                          <i class="fas fa-user-check"></i>
                      </button>
                  </div>
              </div>

     <div>
         </div>
         </div>
    </div>
</body>
</html>
<script>
    $("#sampleBtn").click(function () {
        window.location.href = '@Url.Action("Create", "Samples")';

    });
     $("#sampleBtn1").click(function () {
        window.location.href = '@Url.Action("Index", "Samples")';

    });

</script>